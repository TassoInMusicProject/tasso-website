	Che nac--que_in me* da gli_a-mo-ro--si rag*--gi,		=Fm
	Men--tre_io gio-i*--va_il se--no_ap-ren--do_a l'au*--ra.	=Bm
@MVERSE:
	O lau--ri,_o pal*--me,_o--ve gia-cen--do_a l'au*--ra		=Bm
	Per dol-cez--za lan-gui*--va;_o bel--la fon*--te,		=DM
