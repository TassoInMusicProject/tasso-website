@@BEGIN:	VERSEDATA
@SOLERTI:	244
@CATALOGNUM:	Trm0244
@TITLE:		Qual cervo errando suole
@GENRE:		madrigale
@MANUSRC:	E<sub>1</sub>, F<sub>2</sub>, V<sub>13/E1</sub>
@PRINTSRC:	28, 48, 54, 141, 166, 169
@@BEGIN:	VERSES
@ABOUTIT:	[TIRSI E LICORI]
@ABOUTEN:	[THYRSIS AND LICORI]
@EDITOR:	Solerti, Angelo
@EDITION_TITLE: Le Rime di Torquato Tasso
@EDITION_LOC:	Bologna
