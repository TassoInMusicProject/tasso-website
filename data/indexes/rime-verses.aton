@GENRE:		madrigale
@MANUSRC:	none
@PRINTSRC:	22, 22a), 26, 31, 35, 69, 93, 141, 145, 166, 167, 169
@@BEGIN:	VERSES
@ABOUTIT:	
@ABOUTEN:	
@EDITOR:	Solerti, Angelo
@EDITION_TITLE: Le Rime di Torquato Tasso
@EDITION_LOC:	Bologna
@EDITION_PUB:	Romagnoli-Dall’Acqua
