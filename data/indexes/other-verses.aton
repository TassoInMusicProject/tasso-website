@@BEGIN:	VERSEDATA
@CATALOGNUM:	Tec1001
@ECLOGA:        I
@VERSE:		1
@TITLE:		Dimmi, mesto pastore	
@MANUSRC:	F1
@PRINTSRC:	
@@BEGIN:	VERSES
@ABOUTIT:	Licori
@ABOUTEN:	Lycoris
