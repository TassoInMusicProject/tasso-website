	Sov--ra le stel*--le_il mio non pi--gro_in-ge*--gno.	=Am
@MVERSE:
	Las--so!_e co-no*--sco ben quan--to_i--o dis*--si	=C+m
	Fu vo--ce d'uom* cui ne' tor-men--ti_a-strin*--ga	=Dm
