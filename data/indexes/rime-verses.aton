@GENRE:		madrigale
@MANUSRC:	none
@PRINTSRC:	28, 49, 141, 166, 169
@@BEGIN:	VERSES
@ABOUTIT:	[TIRSI E LICORI]
@ABOUTEN:	[THYRSIS AND LYCORIS]
@EDITOR:	Solerti, Angelo
