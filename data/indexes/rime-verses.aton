@CATALOGNUM:	Trm0303
@TITLE:		Sarai termine ancora
@GENRE:		madrigale
@MANUSRC:	E<sub>1</sub>, F<sub>2</sub>
@PRINTSRC:	27, 48, 54, 70, 141, 166, 169
@@BEGIN:	VERSES
@ABOUTIT:	
@ABOUTEN:	
@EDITOR:	Solerti, Angelo
@EDITION_TITLE: Le Rime di Torquato Tasso
@EDITION_LOC:	Bologna
