	Por--ta lim--pi-do rio* suoi dol--ci_hu-mo*--ri;		=AM
@MVERSE:
	O--ve per--si, ver-mi*--gli_e bian--chi fio*--ri		=A+M
	Ren--don va--go_il ter-re*--no_e di-let-to*--so;		=BM
	O--ve fra_'l crin* de gli_ar-bo-scei-- fron-do*--so		=Bm
