@@BEGIN:	ECLOGHE
@CATALOGNUM:	Tec1001
@ECLOGA:        I
@VERSE:		1
@TITLE:		Dimmi, mesto pastore	
@MANUSRC:	
@PRINTSRC:	
@@BEGIN:	
@ABOUTIT:	Licori
@ABOUTEN:	Lycoris
@EDITOR:	
@EDITION_TITLE: 
@EDITION_LOC:	
@EDITION_PUB:	
@EDITION_DATE:  
@EDITION_VOL:	
@EDITION_PAGE:  
