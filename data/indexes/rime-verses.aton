	Vol--si, di fan*--go_a-sper--se,_ad u--mil se*--gno.	=Am
@MVERSE:
	Or, che può ge*--lo d'o-no-ra--to sde*--gno		=A+m
	Spe--gner la fa*--ce_e quel-l'ar-dor-- mor-ta*--le,	=Bm
