@@BEGIN:	VERSEDATA
@SOLERTI:	254
@CATALOGNUM:	Trm0254
@ATTRIBUTION:	not secure
@TITLE:		Amor, ch'aspro tormento
@GENRE:		madrigale
@MANUSRC:	none
@PRINTSRC:	27, 70, 141, 166, 169
@@BEGIN:	VERSES
@ABOUTIT:	
@ABOUTEN:	
@EDITOR:	Solerti, Angelo
@EDITION_TITLE: Le Rime di Torquato Tasso
@EDITION_LOC:	Bologna
