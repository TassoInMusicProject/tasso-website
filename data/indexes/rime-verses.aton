@@BEGIN:	VERSEDATA
@SOLERTI:	308
@CATALOGNUM:	Trm0308
@TITLE:		Soavissimo bacio
@GENRE:		madrigale
@MANUSRC:	E<sub>2</sub>, Flc<sub>2</sub>, V<sub>7</sub>
@PRINTSRC:	none
@@BEGIN:	VERSES
@ABOUTIT:	
@ABOUTEN:	
@EDITOR:	Solerti, Angelo
@EDITION_TITLE: Le Rime di Torquato Tasso
