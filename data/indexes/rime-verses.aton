	Nel se-ren-- di due lu*--ci_ar-den--ti_ed al*--me,		=BM
	Mil--le fa-mo*--se_in-se--gne_e mil--le pal*--me		=Bm
	Spie-gan--do_in un se-re*--no_e chia--ro vi*--so;		=AM
@MVERSE:
	Quan--do ri-vol*--to_à me, ch'in-ten--to_e fi*--so		=Am
	Mi-ra--va le sue ric*--che_e ca--re sal*--me: &mdash;		=BM
	Or can--ta &mdash; dis*--se &mdash; co--me_i co--ri_e l'al*--me	=Bm
	E_'l tuo me-des*--mo_an-co--ra_ab--bia con-qui*--so;		=Am
@MVERSE:
	Né s'o--da ri-so-nar*-- l'ar--me di Mar*--te			=C+M
	La vo--ce tua, ma l'al*--ta_e chia--ra glo*--ria,		=DM
	E_i di-vin-- pre--gi no*--stri_e di co-ste*--i. &mdash;		=EM
