	Di va--go_af-fet*--to_il ci--glio_a-dor--na_e ve*--ste.		=Am
@MVERSE:
	Ma non av-vien*-- pe-rò-- ch'in lei si de*--ste			=A+m
	Al-cun-- bre--ve do-lor*-- de' miei tor-men*--ti,		=BM
	An--zi la ce*--tra_e_i miei non roz--zi_ac-cen*--ti,		=Bm
	E me di-sprez*--za_e le mie vo--glie_o-ne*--ste.		=Am
@MVERSE:
	Né pie-tà_è-- ve*--ra ne' be--gli_oc--chi_ac-co*--glie		=Cm
	Ma cru-del-tà*,-- ch'in tal sem-bian--za_or mo*--stri,		=Dm
