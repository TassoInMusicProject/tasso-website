@@BEGIN:	VERSEDATA
@SOLERTI:	326
@CATALOGNUM:	Trm0326
@TITLE:		Lunge da gli occhi vostri
@GENRE:		madrigale
@MANUSRC:	F<sub>2</sub>
@PRINTSRC:	27, 70, 141, 145, 166, 169
@@BEGIN:	VERSES
@ABOUTIT:	
@ABOUTEN:	
@EDITOR:	Solerti, Angelo
@EDITION_TITLE: Le Rime di Torquato Tasso
@EDITION_LOC:	Bologna
