	Ol--tre l'u-sa*--to_e lam-peg-giar-- tre-man*--ti,		=Bm
	Co--me ne gli_oc*--chi de' cor-te--si_a-man*--ti		=Bm
	Noi ri-mi-riam*-- ta-lor-- vi--ve fa-cel*--le.			=Am
@MVERSE:
	A-man-- for--se là su*--so,_o pur son el*--le			=A+M
	Pie-to--se_a' no--stri_af-fan*--ni,_a' no--stri pian*--ti?	=BM
	Men--tre scor--gon- le_in-si*--die_e_i pas--si_er-ran*--ti	=BM
	Là do--ve_al--tri d'A-mor*-- go--da_e fa-vel*--le?		=AM
@MVERSE:
	Cor-te--si lu*--ci, se Le-an--dro_in ma*--re			=Cm
	O tra-vï-a*--to pe-re-grin-- fos-s'i*--o			=Dm
	Non mi sa-re*--ste di soc-cor--so_a-va*--re:			=Cm
