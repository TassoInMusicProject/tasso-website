	Dep-re-dò,-- sus-sur-ran*--do,_a--pe_in-ge-gno*--sa;	=AM
@MVERSE:
	E ne' la--bra nu-dria*-- l'au--ra_a-mo-ro*--sa		=A+M
	Al Sol de gli_oc--chi suoi* per-pe--tui fio*--ri,	=BM
	E vo-lan--do_a' dol-cis*--si-mi co-lo*--ri		=BM
